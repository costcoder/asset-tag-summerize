{
  "name": "@costcode/atsum",
  "version": "1.1.8",
  "description": "Tool that scans and summarizes assets across multiple AWS accounts.",
  "main": "./dist/main.js",
  "bin": {
    "atsum": "./dist/main.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "b": "tsc -b tsconfig.json --pretty false",
    "build": "tsc -b tsconfig.json --pretty false",
    "postbuild": "node -e \"const fs = require('fs'); const file = './dist/main.js'; const content = fs.readFileSync(file, 'utf8'); fs.writeFileSync(file, '#!/usr/bin/env node\\n' + content);\"",
    "rb": "tsc -b tsconfig.json --pretty false -f"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:costcoder/assets-scanner.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@aws-sdk/client-ec2": "3.915.0",
    "@aws-sdk/client-s3": "3.916.0",
    "@aws-sdk/client-sts": "3.916.0",
    "@aws-sdk/client-rds": "3.916.0",
    "@aws-sdk/client-lambda": "3.916.0",
    "@aws-sdk/client-dynamodb": "3.916.0"
  },
  "devDependencies": {
    "@types/node": "22.5.4",
    "typescript": "5.9.3"
  }
}

{
  "name": "@costcode/atsum",
  "version": "1.2.0",
  "description": "Tool that scans and summarizes assets across multiple AWS accounts.",
  "main": "./dist/main.js",
  "bin": {
    "atsum": "./dist/main.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "b": "tsc -b tsconfig.json --pretty false",
    "build": "tsc -b tsconfig.json --pretty false",
    "postbuild": "node -e \"const fs = require('fs'); const file = './dist/main.js'; let content = fs.readFileSync(file, 'utf8'); if (!content.startsWith('#!/usr/bin/env node')) { content = '#!/usr/bin/env node\\n' + content; fs.writeFileSync(file, content); }\"",
    "rb": "tsc -b tsconfig.json --pretty false -f"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:costcoder/assets-scanner.git"
  },
  "license": "SEE LICENSE IN LICENSE",
